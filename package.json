{
  "name": "serverless-admin-system",
  "version": "0.1.0",
  "scripts": {
    "bootstrap": "node scripts/bootstrap.js",
    "docker:up": "cd backend && npm run docker:up",
    "dev:be": "cd backend && npm run dev",
    "dev:fe": "cd frontend && npm run dev",
    "deploy": "serverless deploy --all",
    "deploy:fe": "serverless deploy --target=./frontend",
    "deploy:be": "serverless deploy --target=./backend",
    "deploy:be:layer": "serverless deploy --target=./backend/layer",
    "deploy:db": "serverless deploy --target=./db",
    "deploy:vpc": "serverless deploy --target=./vpc"
  },
  "license": "MIT",
  "description": "Admin system developed by [Serverless Components](https://github.com/serverless/components)",
  "keywords": [
    "admin-system",
    "serverless",
    "serverless-framework",
    "serverless-components"
  ],
  "author": "yugasun",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serverless-plus/serverless-admin-system.git"
  },
  "bugs": {
    "url": "https://github.com/serverless-plus/serverless-admin-system/issues"
  },
  "homepage": "https://github.com/serverless-plus/serverless-admin-system#readme"
}
